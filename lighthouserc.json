{
    "ci": {
      "collect": {
        "url": ["http://localhost:4173"],
        "startServerCommand": "bun run serve-prod",
        "startServerReadyPattern": "Server is ready",
        "numberOfRuns": 1,
        "settings": {
          "chromeFlags": "--no-sandbox"
        }
      },
      "assert": {
        "assertions": {
          "categories:pwa": ["error", { "minScore": 1 }]
        }
      }
    }
  }

  